<!DOCTYPE html>
<html lang="en">
<% include header %>
<body>
<% include navbar %>
<div class="content">
<div class="data">
<div class="row">
<div class="head_text">Name</div>
<div class="head_text">Mobile</div>
<div class="head_text">Action</div>
</div>
<%val.response.forEach(function(item) { %>
<div class="row">
   <div class="data_text"><%= item.empname %>
   <%if(item.fineowner=='Yes'){%>
   (Fine Owner)
   <%}%>
    </div>
     <div class="data_text"> <script>
	document.write('<a href="tel:' + '<%= item.mobile %>' + '">Call</a>');
</script></div>
     <div class="data_text">
      <%if(request.session.fineowner){ %>
      <script>
	document.write('<a href="/addfine?' + '<%= item.empname %>' + '">Add Fine</a>');
</script>
<%}%>
</div>
     </div>
    <% }); %>
 </div>   
    </div>
    <% include footer %>
</body>
</html>