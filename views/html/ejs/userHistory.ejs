<!DOCTYPE html>
<html lang="en">
<% include header %>
<body>
<% include navbar %>
<div class="content">
<div class="data" >
<div class="row">
<div class="head_text">Name</div>
<div class="head_text">Amount</div>
<div class="head_text">Date</div>
<div class="head_text">Reason</div>
<div class="head_text">Type</div>
<div class="head_text">Paid Through</div>
<div class="head_text">More Info</div>
</div>
<%val.response.forEach(function(item) { %>
<div class="row">
<div class="data_text"> <%= item.person %> </div>
<div class="data_text"><%= item.amount %></div>
<div class="data_text"><%= item.lending_date.toDateString() %></div>
<div class="data_text"> <%= item.title %> </div>
<div class="data_text"> <%= item.type %> </div>
<div class="data_text"> <%= item.payment_mode %> </div>
<div class="data_text"> <%= item.comment %> </div>
 </div>
  <% }); %>
    </div>
</div>    
    <% include footer %>
</body>
</html>